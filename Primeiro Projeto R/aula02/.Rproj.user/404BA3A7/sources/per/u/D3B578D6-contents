# Criando Objetos
c <-  100
d <- 200
a <- c

# Vetor permite armazenar um conjunto de dados da mesma classe
vtexto <- c("Nome", "Sobrenome", "Palmeiras", "Brasil")
vtexto

vnumero <- c(10,100,25,4)
vnumero

valor <-  c(15.35,45.99,9.98,10.05)
valor

id <-  c(1,2,3,4)
id

# Data Frame

dados <-  data.frame(id = id, numero = vnumero, texto = vtexto, valor = valor)
dados


